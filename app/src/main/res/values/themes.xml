        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textColor">@color/white60</item>
    </style>

    <style name="errorText" parent="TextAppearance.AppCompat">
        <item name="android:textSize">10sp</item>
        <item name="android:fontFamily">@font/inter_light</item>
        <item name="android:textColor">@color/red</item>
    </style>

    <style name="EditTextDefault" parent="Widget.AppCompat.EditText">
