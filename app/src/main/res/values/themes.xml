        <item name="android:layout_height">@dimen/dp40</item>
        <item name="android:layout_width">@dimen/dp40</item>
        <item name="android:padding">@dimen/dp12</item>
        <item name="android:layout_marginStart">@dimen/dp20</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="SettingsArrowRight">
        <item name="android:src">@drawable/ic_arrow_right</item>
        <item name="android:layout_height">@dimen/dp12</item>
        <item name="android:layout_width">@dimen/dp12</item>
        <item name="android:layout_marginEnd">@dimen/dp20</item>
        <item name="android:ellipsize">end</item>
        <item name="android:contentDescription">@null</item>

    </style>





</resources>