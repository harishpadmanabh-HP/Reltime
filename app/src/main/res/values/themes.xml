    <style name="EditTextDefaultSignup" parent="Widget.AppCompat.EditText">
        <item name="android:layout_marginStart">@dimen/dp16</item>
        <item name="android:layout_marginEnd">@dimen/dp16</item>
        <item name="android:layout_width">@dimen/dp0</item>
        <item name="android:layout_height">@dimen/dp0</item>
        <item name="android:background">@drawable/basic_corner</item>
        <item name="android:textColor">@color/white80</item>
        <item name="android:paddingStart">@dimen/dp10</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">@dimen/sp16</item>
    </style>

    <style name="SettingsEditText" parent="Widget.AppCompat.EditText">
        <item name="android:layout_marginStart">@dimen/dp16</item>
        <item name="android:layout_marginEnd">@dimen/dp16</item>
        <item name="android:layout_width">@dimen/dp0</item>
        <item name="android:layout_height">@dimen/dp50</item>
        <item name="android:background">@drawable/basic_corner</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:paddingStart">@dimen/dp10</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">@dimen/sp16</item>
