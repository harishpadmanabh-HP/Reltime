<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/basic_corner"
    android:paddingBottom="@dimen/dp25">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_divider_v1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_divider_v2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.28" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_divider_v3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_divider_v4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gd_divider_h1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <!-- End of guidelines-->

    <TextView
        android:id="@+id/tv_title"
        style="@style/SettingsProfilePrimaryTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp23"
        android:text="@string/settings_profile_address_label"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@id/gd_divider_v1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_address_label1"
        style="@style/SettingsProfileSecondaryTextView"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp65"
        android:text="@string/settings_profile_address_label1"
        android:textSize="@dimen/sp12"
        app:layout_constraintEnd_toStartOf="@id/gd_divider_v2"
        app:layout_constraintStart_toStartOf="@id/gd_divider_v1"
        app:layout_constraintTop_toTopOf="@id/gd_divider_h1" />

    <TextView
        android:id="@+id/tv_address_label2"
        style="@style/SettingsProfileSecondaryTextView"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"
        android:text="@string/settings_profile_address_label2"
        android:textSize="@dimen/sp12"
        app:layout_constraintEnd_toEndOf="@id/gd_divider_v2"
        app:layout_constraintStart_toStartOf="@id/gd_divider_v1"
        app:layout_constraintTop_toBottomOf="@+id/v_d1" />

    <TextView
        android:id="@+id/tv_city_label"
        style="@style/SettingsProfileSecondaryTextView"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"
        android:text="@string/settings_profile_address_city"
        android:textSize="@dimen/sp12"
        app:layout_constraintEnd_toStartOf="@id/gd_divider_v2"
        app:layout_constraintStart_toStartOf="@id/gd_divider_v1"
        app:layout_constraintTop_toBottomOf="@+id/v_d2" />


    <!--  End of label textviews-->

    <TextView
        android:id="@+id/tv_state_label"
        style="@style/SettingsProfileSecondaryTextView"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/settings_profile_address_state"
        android:textSize="@dimen/sp12"
        app:layout_constraintEnd_toStartOf="@id/gd_divider_v2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@id/gd_divider_v1"
        app:layout_constraintTop_toBottomOf="@+id/v_d3" />

    <TextView
        android:id="@+id/tv_address"
        style="@style/SettingsProfilePrimaryTextView"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dp10"
        android:ellipsize="end"
        android:maxLines="2"
        android:singleLine="true"
        android:text=""
        android:textSize="@dimen/sp12"
        app:layout_constraintBottom_toBottomOf="@+id/tv_address_label1"
        app:layout_constraintEnd_toEndOf="@id/gd_divider_v4"
        app:layout_constraintStart_toStartOf="@id/gd_divider_v3"
        app:layout_constraintTop_toTopOf="@+id/tv_address_label1" />

    <TextView
        android:id="@+id/tv_address_two"
        style="@style/SettingsProfilePrimaryTextView"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dp10"
        android:ellipsize="end"
        android:maxLines="2"
        android:singleLine="true"
        android:text=""
        android:textSize="@dimen/sp12"
        app:layout_constraintBottom_toBottomOf="@+id/tv_address_label2"
        app:layout_constraintEnd_toEndOf="@id/gd_divider_v4"
        app:layout_constraintStart_toStartOf="@id/gd_divider_v3"
        app:layout_constraintTop_toTopOf="@+id/tv_address_label2" />

    <TextView
        android:id="@+id/tv_city"
        style="@style/SettingsProfilePrimaryTextView"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dp10"
        android:ellipsize="end"
        android:maxLines="2"
        android:singleLine="true"
        android:text=""
        android:textSize="@dimen/sp12"
        app:layout_constraintBottom_toBottomOf="@+id/tv_city_label"
        app:layout_constraintEnd_toEndOf="@id/gd_divider_v4"
        app:layout_constraintStart_toStartOf="@id/gd_divider_v3"
        app:layout_constraintTop_toTopOf="@+id/tv_city_label" />

    <TextView
        android:id="@+id/tv_state"
        style="@style/SettingsProfilePrimaryTextView"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:maxLines="2"
        android:singleLine="true"
        android:text=""
        android:textSize="@dimen/sp12"
        app:layout_constraintBottom_toBottomOf="@+id/tv_state_label"
        app:layout_constraintEnd_toEndOf="@id/gd_divider_v4"
        app:layout_constraintStart_toStartOf="@+id/gd_divider_v3"
        app:layout_constraintTop_toTopOf="@+id/tv_state_label" />

    <TextView
        android:id="@+id/tv_edit_label"
        style="@style/SettingsProfilePrimaryTextView"
        android:layout_width="@dimen/dp0"
        android:layout_height="wrap_content"
        android:text="@string/settings_profile_edit_label"
        android:textColor="@color/darkblue"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_title"
        app:layout_constraintEnd_toEndOf="@id/gd_divider_v4"
        app:layout_constraintStart_toStartOf="@id/gd_divider_v3"
        app:layout_constraintTop_toTopOf="@+id/tv_title" />


    <View
        android:id="@+id/v_d1"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp1"
        android:layout_marginTop="16dp"
        android:background="@color/gray50"
        app:layout_constraintEnd_toEndOf="@id/gd_divider_v4"
        app:layout_constraintStart_toStartOf="@id/gd_divider_v1"
        app:layout_constraintTop_toBottomOf="@+id/tv_address" />

    <View
        android:id="@+id/v_d2"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp1"
        android:layout_marginTop="16dp"
        android:background="@color/gray50"
        app:layout_constraintEnd_toEndOf="@id/gd_divider_v4"
        app:layout_constraintStart_toStartOf="@id/gd_divider_v1"
        app:layout_constraintTop_toBottomOf="@+id/tv_address_two" />

    <View
        android:id="@+id/v_d3"
        android:layout_width="@dimen/dp0"
        android:layout_height="@dimen/dp1"
        android:layout_marginTop="8dp"
        android:background="@color/gray50"
        app:layout_constraintEnd_toEndOf="@id/gd_divider_v4"
        app:layout_constraintStart_toStartOf="@id/gd_divider_v1"
        app:layout_constraintTop_toBottomOf="@+id/tv_city" />


</androidx.constraintlayout.widget.ConstraintLayout>